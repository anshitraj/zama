
# Sepolia RPC URL (Get from Alchemy or Infura)
VITE_SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Smart Contract Address (Deploy ConfidentialExpenses contract and paste address)
VITE_CONTRACT_ADDRESS=0x...

# IPFS API Configuration (Pinata or nft.storage)
VITE_IPFS_API_KEY=your_pinata_jwt_or_nftstorage_key
VITE_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/

# FHE Coprocessor URL (Optional - for homomorphic aggregation)
VITE_COPROCESSOR_URL=http://localhost:8545/fhe-compute

# WalletConnect Project ID (Get from https://cloud.walletconnect.com/)
VITE_WALLETCONNECT_PROJECT_ID=your_project_id